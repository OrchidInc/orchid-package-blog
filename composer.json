{
  "name": "orchidinc/orchid-blog",
  "description": "Blog plugin for Orchid Admin Panel",
  "keywords": [
    "orchid blog",
    "orchid posts"
  ],
  "license": "MIT",
  "type": "plugin",
  "homepage": "https://github.com/OrchidInc/orchid-package-blog",
  "authors": [
    {
      "name": "Pralhad Kumar Shrestha",
      "email": "pralhadkumarshrestha@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "mr-vaco/orchid-status-plugin": "0.*"
  },
  "autoload": {
    "psr-4": {
      "OrchidInc\\Orchid\\Blog\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "OrchidInc\\Orchid\\Blog\\BlogServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
